	<HediffDef>
		<defName>Acid_Yadarit</defName>
		<hediffClass>HediffWithComps</hediffClass>
		<defaultLabelColor>(0.6, 1.0, 0.6)</defaultLabelColor>
		<label>Yadarit toxicity</label>
		<labelNoun>an Yadarit toxicity</labelNoun>
		<description>Blood supply has worsened</description>
		<lethalSeverity>1</lethalSeverity>
		<makesSickThought>true</makesSickThought>
		<scenarioCanAdd>true</scenarioCanAdd>
		<taleOnVisible>ToxicityRevealed</taleOnVisible>
		<comps>
			<li Class="HediffCompProperties_Immunizable">
				<severityPerDayNotImmune>-0.08</severityPerDayNotImmune>
			</li>
			<li Class="SK.HediffCompProperties_FleshOnly"/>
		</comps>
		<stages>
			<li>
				<label>extreme</label>
				<becomeVisible>false</becomeVisible>
			</li>
			<li>
				<label>extreme</label>
				<minSeverity>0.04</minSeverity>
				<statOffsets>
               <Flammability>1.5</Flammability>
                 </statOffsets>
				<capMods>
					<li>
						<capacity>BloodFiltration</capacity>
						<offset>-0.02</offset>
					</li>
					   <li>
                       <capacity>Consciousness</capacity>
                        <setMax>0.1</setMax>
                        </li>
                     <li>
                       <capacity>Breathing</capacity>
                         <setMax>0.1</setMax>
                       </li>
                      <li>
                      <capacity>BloodPumping</capacity>
                         <setMax>0.1</setMax>
                       </li>
		           </capMods>
			</li>
		</stages>
	</HediffDef>
	
	
	
	
		
		
	
	<HediffDef>
    <defName>ElectricBurn</defName>
    <label>ElectricBurn</label>
	<hediffClass>HediffWithComps</hediffClass> 
    <defaultLabelColor>(0.5, 0.7, 0.3)</defaultLabelColor> 
    <description>ElectricBurn</description>
	    <minSeverity>0</minSeverity>
		<maxSeverity>1</maxSeverity>
		<lethalSeverity>1</lethalSeverity>
		<makesSickThought>true</makesSickThought>
		<scenarioCanAdd>false</scenarioCanAdd>
		<isBad>true</isBad>
		<comps>
		<li Class="HediffCompProperties_Immunizable">
				<severityPerDayNotImmune>-0.003</severityPerDayNotImmune>
			</li>
		</comps>
    <stages>
      <li>
        <label>minor</label>
        <becomeVisible>false</becomeVisible>
      </li>
      <li>
        <minSeverity>0.15</minSeverity>
        <label>minor</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.1</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.30</minSeverity>
        <label>moderate</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.2</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.45</minSeverity>
        <label>severe</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.4</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.60</minSeverity>
        <label>extreme</label>
        <lifeThreatening>true</lifeThreatening>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.4</offset>
          </li>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.1</setMax>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
	
 
	

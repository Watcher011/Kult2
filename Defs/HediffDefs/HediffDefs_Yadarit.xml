<Defs>

	<HediffDef>
		<defName>YadaritP</defName> 
		<hediffClass>HediffWithComps</hediffClass> 
		<defaultLabelColor>(0.5, 0.7, 0.3)</defaultLabelColor> 
		<label>Yadarit buildup</label> 
		<labelNoun>a Yadarit buildup</labelNoun>
		<description>Immersed in a dream</description>
		<minSeverity>0</minSeverity>
		<maxSeverity>1</maxSeverity>
		<lethalSeverity>1</lethalSeverity>
		<makesSickThought>true</makesSickThought>
		<scenarioCanAdd>false</scenarioCanAdd>
		<isBad>true</isBad>
		<comps>
		<li Class="HediffCompProperties_Immunizable">
				<severityPerDayNotImmune>-0.003</severityPerDayNotImmune>
			</li>
			<li Class="SK.HediffCompProperties_FleshOnly"/>
			  <li Class="HediffCompProperties_DissolveGearOnDeath">
        <fleck>AcidCloud</fleck>
        <moteCount>6</moteCount>
        <moteOffsetRange>0.25~0.75</moteOffsetRange>
        <filth>Filth_Yadarit</filth>
        <sound>DeathAcidifier</sound>
        <injuryCreatedOnDeath>ChemicalBurn</injuryCreatedOnDeath>
        <injuryCount>3~6</injuryCount>
      </li>
		</comps>
		<stages>
			<li>
				<label>initial</label>
				<becomeVisible>false</becomeVisible>
			</li>
			<li>
				<label>initial</label>
				<minSeverity>0</minSeverity>
				<capMods>
					<li>
						<capacity>BloodFiltration</capacity>
						<offset>-0.10</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.010</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.10</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>minor</label>
				<minSeverity>0.1</minSeverity>
				<painOffset>0.2</painOffset>
				<vomitMtbDays>0.1</vomitMtbDays>
				<capMods>
					<li>
						<capacity>BloodFiltration</capacity>
						<offset>-0.20</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.020</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.20</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>YadaritLiver</hediff>
						<partsToAffect>
							<li>Liver</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<label>moderate</label>
				<minSeverity>0.2</minSeverity>
				<vomitMtbDays>0.03</vomitMtbDays>
				<painOffset>0.3</painOffset>
				<capMods>
					<li>
						<capacity>BloodFiltration</capacity>
						<offset>-0.30</offset>
					</li>
						<li>
						<capacity>Consciousness</capacity>
						<offset>-0.030</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.30</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.30</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>EyeYadarit</hediff>
						<partsToAffect>
							<li>Eye</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<label>serious</label>
				<minSeverity>0.4</minSeverity>
				<vomitMtbDays>0.05</vomitMtbDays>
					<painOffset>0.8</painOffset>
				<capMods>
					<li>
						<capacity>BloodFiltration</capacity>
						<offset>-0.35</offset>
					</li>
						<li>
						<capacity>Consciousness</capacity>
						<offset>-0.035</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.35</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-0.35</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
						<hediff>YadaritHeart</hediff>
						<partsToAffect>
							<li>Heart</li>
						</partsToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<label>extreme</label>
				<minSeverity>0.9</minSeverity>
				<vomitMtbDays>0.01</vomitMtbDays>
					<painOffset>0.0</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<setMax>0.1</setMax>
					</li>
					<li>
						<capacity>BloodFiltration</capacity>
						<offset>-1.1</offset>
					</li>
						<li>
						<capacity>Consciousness</capacity>
						<offset>-1.1</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-1.1</offset>
					</li>
					<li>
						<capacity>Breathing</capacity>
						<offset>-1.1</offset>
					</li>
				</capMods>
				 
        
      </li>

		</stages>
	</HediffDef>
	
	 <RecipeDef ParentName="SurgeryFlesh">
    <defName>CureYAd</defName>
    <label>cure Yadarit</label>
    <description>This special medical operation cures blood rot by a massive infusion of bacteriophage mechanites.</description>
    <workerClass>Recipe_RemoveHediff</workerClass>
    <jobString>Curing Yadarit rot.</jobString>
    <workAmount>2000</workAmount>
    <hideBodyPartNames>true</hideBodyPartNames>
    <isViolation>false</isViolation>
    <targetsBodyPart>false</targetsBodyPart>
    <removesHediff>YadaritP</removesHediff>
    <successfullyRemovedHediffMessage>{0} has successfully cured {1}'s blood rot.</successfullyRemovedHediffMessage>
    <skillRequirements>
      <Medicine>5</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>YadaritInhibit</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
   
	<researchPrerequisite>YadaritPR</researchPrerequisite>
  </RecipeDef>
  
  

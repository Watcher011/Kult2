 <ThingDef ParentName="SK_ResourceBase">
		<defName>YadareCell</defName>
		<label>YadareCell</label>
		<description>Compact yadarite battery, due to the specifics, can be used in devices with high power consumption.</description>
		<graphicData>
			<texPath>Things/Item/YadareCell</texPath>
		    <graphicClass>Graphic_Single</graphicClass>
			<drawSize>(0.8,0.4)</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<fillPercent>0.35</fillPercent>
		<stackLimit>5</stackLimit>
		<useHitPoints>true</useHitPoints>
		<techLevel>Spacer</techLevel>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>400</MarketValue>
			<DeteriorationRate>0.02</DeteriorationRate>
			<Flammability>15</Flammability>
			<Mass>0.5</Mass>
			<Bulk>0.5</Bulk>
		</statBases>
		<specialDisplayRadius>3</specialDisplayRadius>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>3.4</explosiveRadius>
				<explosiveDamageType>Flame</explosiveDamageType>
				<explosiveExpandPerStackcount>0.2</explosiveExpandPerStackcount>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<wickTicks>
					<min>5</min>
					<max>10</max>
				</wickTicks>
			</li>
			<li Class="SK.CompProperties_ApplyHediffAround">
				<hediff>YadaritP</hediff>
				<radius>3</radius>
				<severityToAdd>0.005</severityToAdd>
			</li>
		</comps>
		 <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
         <killedLeavings>
          <DangerCore>1</DangerCore>
         </killedLeavings>
		<thingCategories>
			<li>CTextiles</li>
		</thingCategories>
		<researchPrerequisites>
			<li>YadaritePower</li>
		</researchPrerequisites>
	</ThingDef>

 

<Defs>

<ThingDef ParentName="PowerFueled">
		<defName>YadaritVolgerGenerator</defName>
		<label>Yadarit volger plant</label>
		<thingClass>Building</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Power/Vogler</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1,1)</drawSize>
			<shadowData>
				<volume>(1, 0.8, 1)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
		</graphicData>
		 <altitudeLayer>Building</altitudeLayer>
		<minifiedDef>MinifiedThing</minifiedDef>
		<passability>PassThroughOnly</passability>
		<surfaceType>Item</surfaceType>
		<fillPercent>0.9</fillPercent>
		<statBases>
			<MaxHitPoints>40</MaxHitPoints>
			<WorkToBuild>6500</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-60</Beauty>
			<Mass>70</Mass>
		</statBases>
		<tickerType>Normal</tickerType>
		<description>A Fuel Power Plant that produces electricity. Can be fueled with yadarit.</description>
		<size>(1,1)</size>
		<stuffCategories>
			<li>RuggedMetallic</li>
		</stuffCategories>
		<costStuffCount>70</costStuffCount>
		<costList>
			<ReinforcedConcrete>30</ReinforcedConcrete>
			<ComponentIndustrial>15</ComponentIndustrial>
			<Mechanism>10</Mechanism>
		</costList>
		<building>
			<destroySound>BuildingDestroyed_Metal_Medium</destroySound>
			<uninstallWork>450</uninstallWork>
		</building>
		<comps>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_Power">
				<compClass>SK.CompPowerPlantFueled</compClass>
				<basePowerConsumption>-970</basePowerConsumption>
				<transmitsPower>true</transmitsPower>
				<soundAmbientProducingPower>ChemfuelFiredGenerator_Ambience</soundAmbientProducingPower>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>3</glowRadius>
				<glowColor>(217,112,33,0)</glowColor>
			</li>
			<li Class="SK.CompProperties_HeatPusherAdvanced">
				<compClass>SK.CompHeatPusherAdvanced</compClass>
				<heatPerSecond>30</heatPerSecond>
				<heatPushMaxTemperature>75</heatPushMaxTemperature>
			</li>
			<li Class="SK.CompFueled_Properties">
				<canAutoBurn>true</canAutoBurn>
				<operatingTemp>300</operatingTemp>
				<drawfirefuel>false</drawfirefuel>
				<smokeDrawOffset>(0.2,1.5,0.5)</smokeDrawOffset>
				<smokesize>1.4</smokesize>
				<fuelCapacity>10</fuelCapacity>
				<burnFuelMultiplier>60</burnFuelMultiplier>
				<smokeDensity>4</smokeDensity>
				<fuelFilter>
					<thingDefs>
						<li>yadarit</li>
					</thingDefs>
				</fuelFilter>
			</li>
			  <li Class="CompProperties_Battery">
				<storedEnergyMax>500</storedEnergyMax>
				<efficiency>0.1</efficiency>
				<shortCircuitInRain>true</shortCircuitInRain>
			</li>
		</comps>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>Power</designationCategory>
		 <designationHotKey>Misc3</designationHotKey>
		<researchPrerequisites>
			<li>YadariteReclamation</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
		<modExtensions>
			<li Class="SK.Events.AccidentalExplosionExtension">
			</li>
		</modExtensions>
	</ThingDef>






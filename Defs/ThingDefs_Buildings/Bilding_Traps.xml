<Defs>

	<ThingDef ParentName="TurretBase">
		<defName>Turret_1Turret</defName>
		<label>sentry light turret</label>
		<graphicData>
			<texPath>Things/Building/Security/TurretImprovised1</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
			<shadowData>
				<volume>(0.27,0.25,0.27)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
		</graphicData>
		<uiIconPath>Things/Building/Security/TurretImprovised1_Top</uiIconPath>
		<statBases>
			<Flammability>1.0</Flammability>
			<WorkToBuild>800</WorkToBuild>
			<Mass>16</Mass>
			<Bulk>20</Bulk>
		</statBases>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>700</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<description>-----</description>
		<stuffCategories>
			<li>RuggedMetallic</li>
		</stuffCategories>
		<costStuffCount>50</costStuffCount>
		<costList>
			 <Weapon_Parts>10</Weapon_Parts>
			<Electronics>1</Electronics>
			<Mechanism>5</Mechanism>
			<Wire>75</Wire>
		</costList>
		<specialDisplayRadius>1</specialDisplayRadius>
		<designationCategory>Security</designationCategory>
		<building>
			<ai_combatDangerous>false</ai_combatDangerous>
			<turretGunDef>Gun_MiniTurretA</turretGunDef>
			<turretTopDrawSize>4</turretTopDrawSize>
		</building>
		<designationHotKey>Misc1</designationHotKey>
		<tradeTags>
			<li>CE_Turret</li>
		</tradeTags>
		<minifiedDef>MinifiedTurretGun</minifiedDef>
		<thingCategories>
			<li>BuildingsSecurity</li>
		</thingCategories>
		<constructionSkillPrerequisite>12</constructionSkillPrerequisite>
		<modExtensions>
			<li Class="SK.Events.AccidentalExplosionExtension">
			</li>
		</modExtensions>
		<researchPrerequisites>
			<li>Homemede_defense</li>
		</researchPrerequisites>
	</ThingDef>


 

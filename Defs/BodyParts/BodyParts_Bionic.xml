<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	
	<HediffDef ParentName="AddedBodyPartBase">
    <defName>mechandrid</defName>
    <label>mechandrid</label>
    <labelNoun>a bionic mechandrid</labelNoun>
    <description>An installed bionic mechandrid.</description>
    <descriptionHyperlinks><ThingDef>mechandrid</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>mechandrid</spawnThingOnRemoved>
    <woundAnchorRange>0</woundAnchorRange>
    <displayWound>true</displayWound>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.5</partEfficiency>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
	<stages>
			<li>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>0.3</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>0.03</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<offset>0.10</offset>
					</li>
				</capMods>
			</li>	 
		</stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartBionicBase">
    <defName>mechandrid</defName>
    <label>bionic mechandrid</label>
	<graphicData>
			<texPath>Things/Item/BodyPart/mechandrid</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.80</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
    <description>An advanced artificial eye. With its biogel nerve-link, internal signal pre-processor, and wide-spectrum sensors, it is better than a biological eye in almost every way. A lattice-dust healing system allows it to recover from damage.</description>
    <descriptionHyperlinks><RecipeDef>Installmechandrid</RecipeDef></descriptionHyperlinks>
    <costList>
     <SealOfPurity>1</SealOfPurity>
      <ComponentSpacer>4</ComponentSpacer>
	  <ComponentAdvanced>4</ComponentAdvanced>
	 <Biomatter>25</Biomatter>
	 <SyntheticFibers>16</SyntheticFibers>
    </costList>
    <thingSetMakerTags><li>RewardStandardMidFreq</li></thingSetMakerTags>
    <statBases>
      <Mass>0.3</Mass>
    </statBases>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>Installmechandrid</defName>
    <label>install bionic mechandrid</label>
    <description>Install a bionic mechandrid.</description>
    <descriptionHyperlinks>
      <ThingDef>mechandrid</ThingDef>
      <HediffDef>mechandrid</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing bionic mechandrid.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>mechandrid</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>mechandrid</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Spine</li>
    </appliedOnFixedBodyParts>
    <addsHediff>mechandrid</addsHediff>
	<skillRequirements>
			<Medicine>15</Medicine>
		</skillRequirements>
  </RecipeDef>


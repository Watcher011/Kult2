	<RecipeDef Name="Make_YadaritcoreBase" Abstract="True">
		<fixedIngredientFilter>
			<thingDefs>
				<li>yadarit</li>
				<li>yadaritRaW</li>
			</thingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>YadaritBasin</li>
		</recipeUsers>
		
		<!-- RecipeDef_UF fields -->
		<temperatureSafe>
			<min>-50</min>
			<max>0</max>
		</temperatureSafe>
		<temperatureIdeal>
			<min>-40</min>
			<max>-5</max>
		</temperatureIdeal>
		<processDays>10</processDays>
		<speedBelowSafe>0.05</speedBelowSafe>
		<speedAboveSafe>0.5</speedAboveSafe>
		<sunFactor>
			<min>1</min>
			<max>1</max>
		</sunFactor>
		<rainFactor>
			<min>1</min>
			<max>1</max>
		</rainFactor>
		<snowFactor>
			<min>1</min>
			<max>1</max>
		</snowFactor>
		<windFactor>
			<min>1</min>
			<max>1</max>
		</windFactor>
		<graphicSuffix>_sin</graphicSuffix>
	</RecipeDef>
	
	<RecipeDef ParentName="Make_YadaritcoreBase" Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>Make_YadaritCore</defName>
		<label>make Yadarit Core</label>
		<description>Make Yadarit core. Takes 4 days to make</description>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>yadarit</li>
					</thingDefs>
				</filter>
				<count>75</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Glass</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentSpacer</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<YadareCell>3</YadareCell>
		</products>
	</RecipeDef>

	
	
	<RecipeDef ParentName="Make_YadaritcoreBase" Class="UniversalFermenterSK.RecipeDef_UF">
